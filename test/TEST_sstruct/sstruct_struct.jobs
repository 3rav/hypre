#!/bin/ksh 
#=============================================================================
# sstruct: compares the struct solvers from calls from sstruct & struct
#          itself.
#=============================================================================

mpirun -np 1 ./struct -n 12 12 12 -solver 1 > struct.out.1
mpirun -np 1 ./sstruct -in sstruct.in.struct -solver 201 > struct.out.201

mpirun -np 1 ./struct -n 12 12 12 -solver 0 > struct.out.0
mpirun -np 1 ./sstruct -in sstruct.in.struct -solver 200 > struct.out.200


