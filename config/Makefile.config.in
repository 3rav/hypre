@SET_MAKE@

SHELL = /bin/sh

srcdir = @srcdir@
VPATH = @srcdir@


.SUFFIXES:
.SUFFIXES: .o .f .c .C .cxx

.f.o:
	$(F77) $(FFLAGS) -c $< 
.c.o:
	$(CC) $(CFLAGS) $(CPPFLAGS) -c $< 
.C.o:
	$(CXX) $(CXXFLAGS) $(CPPFLAGS) -c $<
.cxx.o:
	$(CXX) $(CXXFLAGS) $(CPPFLAGS) -c $<


F77       = @F77@
FFLAGS    = @FFLAGS@ @F77FLAGS@

CC        = @CC@
CFLAGS    = @CFLAGS@

CXX       = @CXX@
CXXFLAGS  = @CXX_FLAGS@

CPPFLAGS  = @DEFS@

AR	  = @AR@ -rcu
RANLIB    = @RANLIB@

LDFLAGS   = @LDFLAGS@
LDLIBS    = @LDLIBS@
